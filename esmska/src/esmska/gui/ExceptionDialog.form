      <AuxValues>
        <AuxValue name="JavaCodeGenerator_InitCodePost" type="java.lang.String" value="errorLabel.addValuedListener(new ValuedListener&lt;JHtmlLabel.Events, String&gt;() {&#xa;    @Override&#xa;    public void eventOccured(ValuedEvent&lt;Events, String&gt; e) {&#xa;        if (e.getEvent() == Events.LINK_CLICKED) {&#xa;            String url = e.getValue();&#xa;            //we must copy code from Actions.getBrowseAction, because Actions&#xa;            //class would initialize MainFrame even when it not may be initialized&#xa;            //now (that would cause problems, because config files may have not &#xa;            //been read yet)&#xa;            if (!Desktop.isDesktopSupported()) {&#xa;                return;&#xa;            }&#xa;            Desktop desktop = Desktop.getDesktop();&#xa;            try {&#xa;                logger.fine(&quot;Browsing URL: &quot; + url);&#xa;                desktop.browse(new URL(url).toURI());&#xa;            } catch (Exception ex) {&#xa;                logger.log(Level.WARNING, &quot;Could not browse URL: &quot; + url, ex);&#xa;            }&#xa;        }&#xa;    }&#xa;});"/>
